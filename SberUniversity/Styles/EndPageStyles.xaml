<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:SberUniversity">

    <Style x:Key="EndContainer">
        <Setter Property="Canvas.Width" Value="795"/>
        <Setter Property="Canvas.Height" Value="1156"/>
        <Setter Property="Canvas.Margin" Value="142,0,142,382"/>
    </Style>
    <Style x:Key="EndHeader" BasedOn="{StaticResource Header1}">
        <Setter Property="TextBlock.Height" Value="170"/>
        <Setter Property="TextBlock.Width" Value="795"/>
        <Setter Property="TextBlock.TextWrapping" Value="Wrap"/>
        <Setter Property="TextBlock.TextAlignment" Value="Center"/>
        <Setter Property="Canvas.Left" Value="0"/>
    </Style>
    <Style x:Key="EndText" BasedOn="{StaticResource Text1}">
        <Setter Property="TextBlock.Height" Value="45"/>
        <Setter Property="TextBlock.Width" Value="795"/>
        <Setter Property="Canvas.Top" Value="233"/>
    </Style>
    
    <Style x:Key="EndQRContainer1" BasedOn="{StaticResource QRContainer}">
        <Setter Property="Canvas.Background" Value="#33FFFFFF"/>
        <Setter Property="Canvas.Clip">
            <Setter.Value>
                <RectangleGeometry RadiusX="36" RadiusY="36" Rect="0,0,655,263"/>
            </Setter.Value>
        </Setter>
        <Setter Property="Canvas.Top" Value="373"/>
    </Style>

    <Style x:Key="EndQRContainer2" BasedOn="{StaticResource EndQRContainer1}">
        <Setter Property="Canvas.Top" Value="676"/>
    </Style>
    <Style x:Key="EndButton" BasedOn="{StaticResource StartButton}">
        <Setter Property="Canvas.Top" Value="1034" />
        <Setter Property="Button.FontWeight" Value="Regular"/>
    </Style>
</ResourceDictionary>